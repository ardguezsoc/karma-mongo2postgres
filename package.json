    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
