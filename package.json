    "make-manifest": "^1.0.4",
    "on-headers": "^1.0.2",
    "optimist": "^0.6.1",
    "optional": "^0.1.4",
    "prepper": "^1.2.0",
    "ramda": "^0.27.0",
    "swagger-endpoint-validator": "^2.0.2",
    "systemic": "^3.3.7",
